{
  "$schema": "http://json-schema.org/schema",
  "id": "gngt-core-ng-add",
  "title": "Gngt Core ng-add schematic",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "Name of the project.",
      "$default": {
        "$source": "projectName"
      }
    },
    "translations": {
      "type": "boolean",
      "default": true,
      "description": "Whether translations module should be set up.",
      "x-prompt": "Set up Gngt Missing Translation Handler as default?"
    }
  },
  "required": []
}