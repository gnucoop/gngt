{
  "compilerOptions": {
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "lib": ["es2015", "dom", "es2016.array.include"],
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "target": "es2015",
    "types": [
      "node"
    ]
  }
}
